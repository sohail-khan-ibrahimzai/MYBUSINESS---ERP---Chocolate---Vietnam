<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="#" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b></b></span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>Hans Corp </b>Abwabal</span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" id="menuToggle" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>

                <script>
                   
                </script>

                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        @if (Session["CurrentUser"] != null)
                        {
                            MYBUSINESS.Models.Employee user = (MYBUSINESS.Models.Employee)HttpContext.Current.Session["CurrentUser"];
                            MYBUSINESS.Models.Employee currentUser = (MYBUSINESS.Models.Employee)HttpContext.Current.Session["CurrentUser"];
                            //if (currentUser.StoreId != null && storeId != 0)
                            //var storeId = Session["StoreId"] as string;
                            var storeId = Session["StoreId"];
                            if (storeId != null)
                            {
                                <li><a href="#" id="closeStore" data-url="/SOSR/ClosePosPopup" data-target="#storeClosingModal">Close POS</a></li>
                            }
                            if (currentUser.EmployeeTypeId == 0)
                            {
                                <li>@Html.ActionLink("Support", "Contact", "Home")</li>
                            }
                            <li>@Html.ActionLink(user.Login, "Edit", "Employees", new { id = 0 }, null)</li>
                            @*<li>@Html.ActionLink("Logout", "Logout", "UserManagement")</li>*@
                            <li>
                                <a href="#" onclick="logout()">Logout</a>
                            </li>
                        }
                    </ul>
                </div>
